{
    "name": "mawuekom/geo-target-ip",
    "description": "A package to geo targeting IP address",
    "keywords": [
        "mawuekom",
        "geo-target-ip"
    ],
    "homepage": "https://github.com/mawueva/geo-target-ip",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ephra\u00efm Seddor",
            "email": "seddorephraim7@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "mawuekom/http-request-wrapper": "^1.4"
    },
    "require-dev": {
        "marcocesarato/php-conventional-changelog": "^1.10",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mawuekom\\GeoTargetIp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mawuekom\\GeoTargetIp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "changelog": "vendor/bin/conventional-changelog --history --commit-all"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "changelog": {
            "source": "changelog.json"
        }
    },
    "version": "1.0.2"
}