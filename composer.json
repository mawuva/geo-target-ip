    "keywords": [
        "mawuekom",
        "geo-target-ip"
    ],
    "homepage": "https://github.com/mawuekom/geo-target-ip",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ephra√Øm Seddor",
            "email": "seddorephraim7@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
